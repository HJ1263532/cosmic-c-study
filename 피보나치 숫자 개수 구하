#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main(void)
{
	while (1)
	{
		int n;
		printf("Enter an integer>> ");
		scanf("%d", &n);
		if (n == 0)
			return 0;
		printf("  Divisors of %d: ", n);
		for (int i = 1; i <= n; i++) {
			if (n % i == 0) {
				printf("%d ", i);
			}
		}
		printf("\n");
	}

	return 0;
}
